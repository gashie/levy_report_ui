<div class="dt-responsive table-responsive">
    <table class="m-b-20">
        <tbody>
            <tr>
                <form role="form" method="post" action="/recon">
                    <td>Start at:</td>
                    <td>
                        <input class="form-control" type="date" name="StartDate" value="{{firstDate}}">
                    </td>
                    <td>End at:</td>
                    <td>
                        <input class="form-control" type="date" name="EndDate" value="{{lastDate}}">
                    </td>
                    <td>
                        <button type="submit" id="submit" class="btn btn-primary btn-sm">Search</button>
                    </td>
                    {{!-- <td>
                        <span class="label label-warning">Data Showing from {{firstDate}} to {{lastDate}}</span>
                    </td> --}}
                </form>
            </tr>

        </tbody>
    </table>
</div>
<span class="label label-warning">Data Showing from {{firstDate}} to {{lastDate}}</span>

<hr />
<div class="row">
    <div class="col-md-6 col-xl-3">
        <div class="card card-social">
            <div class="card-block border-bottom">
                <div class="row align-items-center justify-content-center">
                    <div class="col-auto">
                        {{!-- <i class="fab fa-twitter text-c-info f-36"></i> --}}
                    </div>
                    <div class="col text-right">
                        <h6>{{formatnumber count}}</h6>
                        <h5 class="text-c-info mb-0"><span class="text-muted">Total Transaction Count</span></h5>
                    </div>
                </div>
            </div>

        </div>
    </div>
    <div class="col-md-6 col-xl-3">
        <div class="card card-social">
            <div class="card-block border-bottom">
                <div class="row align-items-center justify-content-center">
                    <div class="col-auto">
                        {{!-- <i class="fab fa-facebook-f text-primary f-36"></i> --}}
                        <i class="fab fa-exchange text-primary f-36"></i>
                    </div>
                    <div class="col text-right">
                        <h6>{{fomartprice TransferAmount}}</h6>
                        <h5 class="text-c-blue mb-0"><span class="text-muted">Total Transfer Amount</span></h5>
                    </div>
                </div>
            </div>

        </div>
    </div>
    <div class="col-md-6 col-xl-3">
        <div class="card card-social">
            <div class="card-block border-bottom">
                <div class="row align-items-center justify-content-center">
                    <div class="col-auto">
                        {{!-- <i class="fab fa-twitter text-c-info f-36"></i> --}}
                    </div>
                    <div class="col text-right">
                        <h6>{{fomartprice TaxableAmount}}</h6>
                        <h5 class="text-c-info mb-0"><span class="text-muted">Total Taxable Amount</span></h5>
                    </div>
                </div>
            </div>

        </div>
    </div>

    <div class="col-md-6 col-xl-3">
        <div class="card card-social">
            <div class="card-block border-bottom">
                <div class="row align-items-center justify-content-center">
                    <div class="col-auto">
                        {{!-- <i class="fab fa-google-plus-g text-c-red f-36"></i> --}}
                    </div>
                    <div class="col text-right">
                        <h6>{{fomartprice ElevyAmount}}</h6>
                        <h5 class="text-c-red mb-0"><span class="text-muted">Total E-levy Amount</span></h5>
                    </div>
                </div>
            </div>

        </div>
    </div>
    <!-- sessions-section start -->
    <div class="col-sm-12">
        <div class="card">
            <div class="card-header">
                <h5 style="color: red;">E-Levy Tax Transactions</h5>
            </div>
            <div class="card-body">
                <div class="dt-responsive table-responsive">
                     {{!-- <button id="button-excel" class="btn btn-warning"> <i class="material-icons mr-1">print</i> Print To Excel</button> --}}
                    <table id="simpletable" class="table table-striped table-bordered nowrap">
                        <thead>
                            <tr>
                                <th><span>#</span></th>
                                  <th><span>Channel</span></th>
                                <th><span>Trans Type</span></th>
                                <th><span>Date</span></th>
                                <th><span>Trans Amount</span></th>
                                <th><span>Taxable Amount </span></th>
                                <th><span>Elevy Amount</span></th>
                                <th><span>Receiver Account</span></th>
                                <th><span>Sender Account</span></th>
                                <th><span>Sender Phone </span></th>
                                <th><span>Channel Ref</span></th>
                                <th><span>Trans Ref</span></th>

                                <th><span>Status</span></th>
                            </tr>
                        </thead>
                        <tbody>

                            {{#each data}}
                              {{#checkIf ElevyAmount '!=' "0" '||' ElevyAmount '>' "0"}}
                            <tr>
                                <td>{{@index}}</td>
                                <td> {{AppName}}</td>
                                <td>{{TransactionType}}</td>
                                <td> {{DateCreated}}</td>
                                <td>{{TransferAmount}}</td>
                                <td>{{TaxableAmount}}</td>
                                <td>{{ElevyAmount}}</td>
                                <td>{{ReceiverAccountNumber}}</td>
                                <td> {{SenderAccountNumber}}</td>
                                <td>{{SenderPhoneNumber}}</td>
                                <td>{{ChannelReference}}</td>
                                <td>{{TransFT}}</td>
                                <td>{{#if Status}}
                                    <span class="badge badge-success">Success</span>
                                    {{else}}
                                    <span class="badge badge-danger">Failed</span>
                                    {{/if}}

                                </td>
                            </tr>
                            {{/checkIf}}
                            {{/each}}
                        </tbody>
                    </table>
                </div>
            </div>
        </div>
    </div>

</div>